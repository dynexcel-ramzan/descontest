<odoo>

      
        <record id="view_shipment_booking_form" model="ir.ui.view">
            <field name="name">payroll.tax.form.view</field>
            <field name="model">payroll.tax</field>
            <field name="arch" type="xml">
                <form string="Payroll">
                    <sheet>
                        <group>
                             <field name="name"/>
                            <field name="date_from"/>
                            <field name="date_to"/>
                            <field name="bank_name"/>
                            <field name="branch_address"/>
                            
<!--                             <field name="value2"/>
                            <field name="description"/> -->
<!--                              <field name="payslip"/> -->
                             <button name="print_report" string="Tax Report" type="object" class="btn-primary"/>
                             <button string="Cancel" class="btn-secondary" special="cancel"/>
                                    
<!--                                
     <button string="Generate" class="btn btn-link" type="object"
                                    name="generate_random_tracking"/> -->
                        </group>
                    </sheet>
                </form>
                 </field>
      </record>
      
         <record model="ir.actions.act_window" id="action_tax_report">
            <field name="name">Tax Certificate</field>
            <field name="res_model">payroll.tax</field>
            <field name="view_id" ref="view_shipment_booking_form"/>
             <field name="target">new</field>
            
        </record>
      
    
        <menuitem action="action_tax_report" id="payroll_report" parent="hr_payroll.menu_hr_payroll_report" sequence="3"/>
    
        
<!--         <menuitem action="action_view_tax_report" id="payroll_report" parent="hr_payroll.menu_hr_payroll_report" sequence="1"/>
  -->
</odoo>