<?xml version="1.0" ?>
<odoo>

         <record id="project_project_form_view_project_timesheet" model="ir.ui.view">
			<field name="name">project.project.form.view.timesheet</field>
			<field name="model">project.project</field>
			<field name="inherit_id" ref="project.edit_project"/>
			<field name="arch" type="xml">
                 <xpath expr="//header" position="inside">
                    <button name="action_fetch_oracle_project" string="Fetch Project" type="object" class="oe_highlight"/>
				 </xpath>
                 <xpath expr="//field" position="after">
					<field name="ora_project_id" invisible="1"/>
		         </xpath>
            </field>
		</record>
        
</odoo>