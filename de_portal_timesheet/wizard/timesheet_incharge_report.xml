<odoo>
  <data>
           <!--Display form view of wizard-->    
        <record id="view_timesheet_report_wizard_form" model="ir.ui.view">
            <field name="name">timesheet.reprot.wizard.form</field>
            <field name="model">timesheet.reprot.wizard</field>
            <field name="arch" type="xml">
                <form string="Timesheet Report">
                    <group>
                        
                       <group>
                        <field name="project_id"  />
                              
                      </group>
                        
                      <group>
                        <field name="date_from" />  
                        <field name="date_to" />
                      </group>                        
                        
                    </group>
                   
                    <footer>
                        <button string='Print' name="check_report" type="object" default_focus="1" class="oe_highlight"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <!--Open the wizard on click of menu-->
        <record id="open_timesheet_report_action" model="ir.actions.act_window">
            <field name="name">Timesheet Report</field>
            <field name="res_model">timesheet.reprot.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
            <field name="view_id" ref="view_timesheet_report_wizard_form"/>
        </record>
      
      
         <menuitem
            id="hr_timesheet_report_menu"
            name="Timesheet Report"
            parent="hr_timesheet.menu_timesheets_reports"
            sequence="4"
            action="open_timesheet_report_action"
        />
      
        
     
  </data>
</odoo>